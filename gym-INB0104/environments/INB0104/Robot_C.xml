<mujoco model="INB0104">
  <statistic center="0.3 0 0.4" extent="1"/>
  
  <include file="actuators/general.xml"/>
  <include file="panda_assets.xml"/>
  <include file="d435i_assets.xml"/>

  <visual>
    <rgba haze="0.15 0.25 0.35 1"/>
    <quality shadowsize="8192"/>
    <global azimuth="120" elevation="-20"/>
     <headlight diffuse="0.6 0.6 0.6" ambient="0.3 0.3 0.3" specular="0 0 0"/>
  </visual>

  <option>
    <flag multiccd="enable"/>
  </option>

  <option actuatorgroupdisable="0"/>

  <asset>
    <!-- <texture type="skybox" builtin="gradient" rgb1="0.3 0.5 0.7" rgb2="0 0 0" width="512" height="3072"/> -->
    <texture type="2d" name="groundplane" builtin="checker" mark="edge" rgb1="0.2 0.3 0.4" rgb2="0.1 0.2 0.3" markrgb="0.8 0.8 0.8" width="50" height="50"/>
    <material name="groundplane" texture="groundplane" texuniform="true"  reflectance="0.2"/>
    <texture name="table" file="table.png" type="2d"/>
    <texture name="plywood" file="plywood.png" type="2d"/>
    <material name="plywood" texture="plywood" specular="1" shininess="1"/>
    <texture name="brick_wall" file="brick_wall.png" type="2d"/>
    <material name="brick_wall" texture="brick_wall" texuniform="true" specular="1" shininess="1"/>
    <texture name="aluminium" file="aluminium.png" type="2d"/>
    <material name="aluminium" texture="aluminium" texuniform="true" specular="1" shininess="1"/>
    <mesh file="cuboid.stl"/>
  </asset>

  <compiler angle="radian" meshdir="assets" texturedir = "textures" autolimits="true"/>
  
  <option integrator="implicitfast"/>

  <worldbody>
  
    <body name="light0" pos="0 0 2.5">
      <light pos="0 0 0" dir="0 0 -1" directional="false" diffuse="0.3 0.3 0.3" ambient="0 0 0"/>
    </body>
    <geom name="floor" size="0 0 0.05" type="plane" rgba="0.74 0.73 0.67 1"/>
    <body name="back_wall" pos="-1 0 2" euler="0 1.57 1.57">
      <geom name="back_wall" type="plane" size="2 2 0.02" material="brick_wall"/> 
    </body>
    <body name="side_wall" pos="1 -2 2" euler="-1.57 0 3.1416">
      <geom name="side_wall" type="plane" size="2 2 0.02" material="brick_wall"/> 
    </body>
    <body name="back_curtain" pos="-0.4 0 1.45" euler="0 1.57 3.1416">
      <geom name="back_curtain" type="plane" size="0.9 0.6 0.02" rgba="0.2 0.2 0.2 1"/> 
    </body>
    <body name="left_curtain" pos="0.2 -0.6 1.65" euler="-1.57 0 3.1416">
      <geom name="left_curtain" type="plane" size="0.6 0.7 0.02" rgba="0.2 0.2 0.2 1"/> 
    </body>
    <body name="right_curtain" pos="0.2 0.6 1.45" euler="1.57 0 3.1416">
      <geom name="right_curtain" type="plane" size="0.6 0.9 0.02" rgba="0.2 0.2 0.2 1"/> 
    </body>
    <body pos='-0.1575 -0.51 0' euler='0 0 0'>
        <include file="workbench.xml"/>
    </body>

    <!-- Camera --> 
    <body name = "cam0" pos='0.96 0 1.25' euler='0 0.8 1.57'>
      <camera name='side_camera' mode="fixed" fovy="57"/> 
    </body>

    <!-- Robot -->
    <body pos='0 0 0.92' euler='0 0 0'>
        <include file="panda_body.xml"/>
    </body>
    
    <body mocap="true" name="panda_mocap" pos="0.1 0.01 1.4">
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.7" size="0.005 0.005 0.005" type="box" group="3"></geom>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.1" size="0.2 0.005 0.005" type="box" group="3"></geom>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.1" size="0.005 0.2 0.001" type="box" group="3"></geom>
            <geom conaffinity="0" contype="0" pos="0 0 0" rgba="0 0.5 0 0.1" size="0.005 0.005 0.2" type="box" group="3"></geom>
    </body>

    <!-- target object -->
    <body name="target_object" pos="0.5 0 0.925" euler="0 0 1.57">
      <freejoint name="target"/>
      <geom type="mesh" mesh="cuboid" rgba="0.95294117647 0.23921568627 0.23921568627 1.0"/> 
    </body>

    <site name="target_site" type="box" pos="0.7 0 0.92" size="0.028 0.028 0.001" rgba="0 0 0 1"/>

  </worldbody>

  <equality>
      <weld body1="panda_mocap" body2="ee_center_body" solimp="0.9 0.95 0.001" solref="0.02 1"/>
  </equality>

  <keyframe>
    <!-- <key name="home" qpos="0 0 0 -1.57079 0 1.57079 -0.7853 0.04 0.04" ctrl="0 0 0 -1.57079 0 1.57079 -0.7853 255"/> -->
    <!-- <key name="home" qpos="0 0 0 -1.57079 0 1.57079 -0.7853 0.04 0.04 0 0 0 0 0 0 0" ctrl="0 0 0 -1.57079 0 1.57079 -0.7853 255"/> -->
    <key name="home" qpos="0.00 0.41 0.00 -1.85 0.00 2.26 0.79 0.04 0.04 0.5 0 0.95 0 0 0 0" ctrl="0 0 0 -1.57079 0 1.57079 -0.7853 255"/>
  </keyframe>

</mujoco>
